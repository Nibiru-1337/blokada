<?xml version="1.0"?>
<resources>
    <string name="notification_on_text">Értesítések</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_on_desc">Ha engedélyezve van, a %s értesítést fog mutatni minden alkalommal, ha egy reklámot szűr.</string>
    <!-- Title of the notification that shows up every time ad is blocked. -->
    <string name="notification_blocked_title">Reklám szűrve</string>
    <!-- Content of the notification that shows up every time ad is blocked. Parameter: blocked host name. -->
    <string name="notification_blocked_text">%s</string>
    <!-- Button to turn off notifications. -->
    <string name="notification_blocked_off">Ne mutasd</string>
    <!-- Button to whitelist a host that was just blocked, visible in a notification. -->
    <string name="notification_blocked_whitelist">Fehérlistára</string>
    <!-- Toast message that shows up once user adds a host to the whitelist, using a button from the notification. -->
    <string name="notification_blocked_whitelist_applied">Kész! Az változásoknak kellhet néhány másodperc.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_disabled">A %s nem fog reklám értesítéseket mutatni.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_enabled">A %s mostantól értesítést fog mutatni, valahányszor egy reklámot blokkol.</string>
    <string name="notification_keepalive_text">Életben tart</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_desc">Amikor engedélyezett, a %s olyan technikákat fog használni, amikkel túlélheti a memóriatakarítást.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_disabled">A %s mostantól nem védett az akkumulátor vagy memória takarékoskodástól.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_enabled">A %s mostantól megvédi magát attól, hogy deaktiválja az operációs rendszer.</string>
    <!-- The ads blocked counter, displayed in the Keep Alive notification. Parameter: ads blocked counter. -->
    <plurals name="notification_keepalive_title">
        <item quantity="one">Egy reklám blokkolva</item>
        <item quantity="other">%d reklám blokkolva</item>
    </plurals>
    <!-- The most recently blocked host, displayed in the Keep Alive notification. Parameter: blocked host name -->
    <string name="notification_keepalive_content">Utolsó: %s</string>
    <!-- Content of the Keep Alive notification, if nothing was blocked yet, as in "Last: nothing yet". -->
    <string name="notification_keepalive_none">eddig semmi</string>
</resources>
