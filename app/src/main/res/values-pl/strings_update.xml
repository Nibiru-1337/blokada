<resources>
    <string name="update_infotext">Aktualizacja jest dostępna! Wybierz ikonkę aktualizacji poniżej, aby kontynuować.</string>
    <string name="update_button">Pobierz aktualizację</string>
    <string name="update_notification_title">Aktualizacja dostępna</string>
