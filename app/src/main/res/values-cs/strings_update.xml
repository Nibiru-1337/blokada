<?xml version="1.0"?>
<resources>
    <string name="update_infotext">Aktualizace je k dispozici! Klepnout na ikonu aktualizace, níže jsou podrobnosti.</string>
    <string name="update_button">Stažení aktualizace</string>
    <string name="update_notification_title">Aktualizace k dispozici</string>
