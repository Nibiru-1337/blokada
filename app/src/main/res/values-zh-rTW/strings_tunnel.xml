    <!-- Parameter: branded app name (Blokada). -->
    <string name="tunnel_hosts_desc">顯示組態為被 %s 阻擋的主機數量。</string>
    <!-- How many hosts is configured to be blocked (the blacklist). Parameter: number of hosts. -->
    <plurals name="tunnel_hosts_count">
        <item quantity="other">%d 主機</item>
    </plurals>
    <string name="tunnel_ads_desc">顯示截至目前為止被阻擋的廣告總數。</string>
    <!-- The counter displayed the total number of ads blocked (in the home screen). Parameter: number of ads blocked. -->
    <plurals name="tunnel_ads_blocked">
