<?xml version="1.0"?>
<resources>
    <string name="notification_on_text">通知</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_on_desc">當啟用後，每當廣告被阻擋時，%s 都會顯示通知。</string>
    <!-- Title of the notification that shows up every time ad is blocked. -->
    <string name="notification_blocked_title">廣告阻擋</string>
    <!-- Content of the notification that shows up every time ad is blocked. Parameter: blocked host name. -->
    <string name="notification_blocked_text">%s</string>
    <!-- Button to turn off notifications. -->
    <string name="notification_blocked_off">不要顯示</string>
    <!-- Button to whitelist a host that was just blocked, visible in a notification. -->
    <string name="notification_blocked_whitelist">白名單</string>
    <!-- Toast message that shows up once user adds a host to the whitelist, using a button from the notification. -->
    <string name="notification_blocked_whitelist_applied">完成！變更可能需要幾秒的時間。</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_disabled">%s 不顯示廣告通知。</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_enabled">%s 現在每當廣告被阻擋都會顯示通知。</string>
    <string name="notification_keepalive_text">持久連接</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_desc">當啟用後，%s 將使用幫助清理殘留記憶體的技術。</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_disabled">%s 不再受電池或記憶體節電的保護。</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_enabled">現在 %s 將自我保護免被作業系統停用。</string>
    <!-- The ads blocked counter, displayed in the Keep Alive notification. Parameter: ads blocked counter. -->
    <plurals name="notification_keepalive_title">
        <item quantity="other">已阻擋 %d 個廣告</item>
    </plurals>
    <!-- The most recently blocked host, displayed in the Keep Alive notification. Parameter: blocked host name -->
    <string name="notification_keepalive_content">上次的：%s</string>
    <!-- Content of the Keep Alive notification, if nothing was blocked yet, as in "Last: nothing yet". -->
    <string name="notification_keepalive_none">還沒有</string>
</resources>
