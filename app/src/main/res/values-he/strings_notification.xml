    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_desc">כאשר פעילה, %s תשתמש בשיטות אשר יעזרו לה לשרוד ניקוי זיכרון(RAM).</string>
    <!-- Parameter: branded app name (Blokada). -->
