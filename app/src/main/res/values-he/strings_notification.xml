    <string name="notification_blocked_whitelist_applied">נשמר! ייתכן והשינויים יופעלו רק בעוד מספר שניות.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_disabled">%s לא תציג התראות על פרסומות.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_enabled">%s לא תציג התראה בכול פעם שפרסומת נחסמה.</string>
