<?xml version="1.0"?>
<resources>
    <string name="notification_on_text">Notificaties</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_on_desc">Indien ingeschakeld, zal %s elke keer een notificatie tonen wanneer een advertentie geblokkeerd is.</string>
    <!-- Title of the notification that shows up every time ad is blocked. -->
    <string name="notification_blocked_title">Adv. geblokkeerd</string>
    <!-- Content of the notification that shows up every time ad is blocked. Parameter: blocked host name. -->
    <string name="notification_blocked_text">%s</string>
    <!-- Button to turn off notifications. -->
    <string name="notification_blocked_off">Niet tonen</string>
    <!-- Button to whitelist a host that was just blocked, visible in a notification. -->
    <string name="notification_blocked_whitelist">Witte lijst</string>
    <!-- Toast message that shows up once user adds a host to the whitelist, using a button from the notification. -->
    <string name="notification_blocked_whitelist_applied">Gereed! Wijzigingen kunnen enkele seconden duren.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_disabled">%s zal geen advertentie notificaties tonen</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_enabled">%s zal elke keer een notificatie tonen wanneer een advertentie geblokkeerd is</string>
    <string name="notification_keepalive_text">Actief blijven</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_desc">Indien ingeschakeld zal %s technieken gebruiken om geheugenopschoning te overleven</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_disabled">%s is niet meer beschermd tegen batterij en geheugenopschoning</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_enabled">%s zal zichzelf beschermen tegen uitschakeling door het besturingssysteem</string>
    <!-- The ads blocked counter, displayed in the Keep Alive notification. Parameter: ads blocked counter. -->
    <plurals name="notification_keepalive_title">
        <item quantity="one">Één adv. geblokkeerd</item>
        <item quantity="other">%d adv. geblokkeerd</item>
    </plurals>
    <!-- The most recently blocked host, displayed in the Keep Alive notification. Parameter: blocked host name -->
    <string name="notification_keepalive_content">Recenste: %s</string>
    <!-- Content of the Keep Alive notification, if nothing was blocked yet, as in "Last: nothing yet". -->
    <string name="notification_keepalive_none">nog niets</string>
</resources>
