<?xml version="1.0"?>
<resources>
    <string name="update_infotext">Er is een update is beschikbaar! Klik op het update icoon hieronder voor meer informatie.</string>
    <string name="update_button">Update downloaden</string>
    <string name="update_notification_title">Update beschikbaar</string>
    <!-- Parameter: new app version, for example 2.0.1 -->
    <string name="update_notification_text">Release %1$s is klaar om te downloaden! Druk om verder te gaan.</string>
    <!-- Header displaying the new app version. Parameters: branded app name, and new app version. For example: NEW! Blokada 2.0.1 -->
    <string name="update_header">NIEUW! %1$s %2$s</string>
    <string name="update_header_noupdate">Geen nieuwe updates</string>
    <string name="update_description">Updates bevatten nieuwe opties, bug fixes en prestatie en batterij optimalisaties. Je kunt deze update nu installeren! In het geval van problemen kun je &lt;a href=\"%1$s\"&gt; ook handmatig dowloaden&lt;/a&gt;.</string>
    <string name="update_description_noupdate">Je huidige geinstalleerde versie is up to date. Updates bevatten nieuwe opties, bug fixes en prestatie en batterij optimalisaties. Je wordt op de hoogte gesteld wanneer er een update beschikbaar is.</string>
    <!-- Technical information about app version and versions of used libraries. Please keep the HTML tags and formatting. It's ok to just copy paste the untranslated string. -->
    <string name="update_header_current">Huidige versie: &lt;b&gt;%1$s %2$s&lt;/b&gt;&lt;br/&gt;core: %3$s&lt;br/&gt;ui: %4$s</string>
    <!-- Feel free to make this string a bit more localised. For example it can say, "This app and its Spanish version is made possible thanks to:" -->
    <string name="update_credits_header">Deze app is een open source project en mede mogelijk gemaakt dankzij:</string>
    <!-- We appreciate you translating this app and welcome you to add your name here - it will show up in the About/Update screen. Please add yourself before everyone, one name per line. If you add a link, your name will be clickable (John Doe: http://my-website.com) -->
    <string name="update_credits">Arjan\nKarsen: http://kar.gs\n... en jij!</string>
    <string name="update_dash_desc">Klik voor update informatie.</string>
    <!-- Displayed in the home screen when there is an update. Keep it short. -->
    <string name="update_dash_available">&lt;b&gt;Update beschikbaar!&lt;/b&gt;</string>
    <!-- Displayed in the home screen whenever there is no update. Keep it short. -->
    <string name="update_dash_uptodate">Bijgewerkt</string>
    <!-- Displayed in menu to open the About/Update screen. -->
    <string name="update_about">Over</string>
    <string name="update_about_desc">Klik om informatie over de app weer te geven.</string>
    <string name="update_force_dash_desc">Klik om op updates te controleren.</string>
    <!-- A button that opens system preferences for this app. -->
    <string name="update_button_appinfo">App details</string>
    <!-- A toast message displayed when the update process is starting. -->
    <string name="update_starting">Starten, een ogenblik geduld aub&#8230;</string>
</resources>
