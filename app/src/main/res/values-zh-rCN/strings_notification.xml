<?xml version="1.0"?>
<resources>
    <string name="notification_on_text">显示通知</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_on_desc">当该选项启用时， %s 将会在每次广告被拦截时显示一条通知。</string>
    <!-- Title of the notification that shows up every time ad is blocked. -->
    <string name="notification_blocked_title">广告已拦截</string>
    <!-- Content of the notification that shows up every time ad is blocked. Parameter: blocked host name. -->
    <string name="notification_blocked_text">%s</string>
    <!-- Button to turn off notifications. -->
    <string name="notification_blocked_off">以后不显示该域名的拦截</string>
    <!-- Button to whitelist a host that was just blocked, visible in a notification. -->
    <string name="notification_blocked_whitelist">加入白名单</string>
    <!-- Toast message that shows up once user adds a host to the whitelist, using a button from the notification. -->
    <string name="notification_blocked_whitelist_applied">已完成！可能要花数秒的时间来使更改生效。</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_disabled">%s 将不会显示拦截广告的通知</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_enabled">%s 将会从现在开始在每次拦截一条广告时推送通知</string>
    <string name="notification_keepalive_text">推送持久通知(内存常驻)</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_desc">当该选项启用时，%s 将会推送持久通知来避免当内存不足时进程被系统终止。</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_disabled">%s 将不会帮您节省空间和电池了。</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_enabled">%s 将会从现在开始保护自身不被操作系统所停用。</string>
    <!-- The ads blocked counter, displayed in the Keep Alive notification. Parameter: ads blocked counter. -->
    <plurals name="notification_keepalive_title">
        <item quantity="other">已拦截 %d 条广告</item>
    </plurals>
    <!-- The most recently blocked host, displayed in the Keep Alive notification. Parameter: blocked host name -->
    <string name="notification_keepalive_content">最后拦截： %s</string>
    <!-- Content of the Keep Alive notification, if nothing was blocked yet, as in "Last: nothing yet". -->
    <string name="notification_keepalive_none">还没有呢</string>
</resources>
