    <plurals name="filter_edit_count">
        <item quantity="other">Содержит %d хостов</item>
        <item quantity="one">Содержит %d хост</item>
        <item quantity="few">Содержит %d хоста</item>
        <item quantity="many">Содержит %d хостов</item>
    </plurals>
