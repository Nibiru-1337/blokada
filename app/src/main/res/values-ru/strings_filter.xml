    <!-- Displayed in the home screen, keep it short. Parameter: number of blacklisted entries. -->
    <plurals name="filter_blacklist_text">
        <item quantity="one">%d в черном списке</item>
        <item quantity="other">%d в черном списке</item>
        <item quantity="few">%d в черном списке</item>
        <item quantity="many">%d в черном списке</item>
    </plurals>
