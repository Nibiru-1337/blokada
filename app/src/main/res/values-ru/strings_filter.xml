    <string name="filter_name_link_unknown">из интернета</string>
    <!-- Parameter: directory of the file. For example: A file from Downloads -->
    <string name="filter_name_file">Файл %s</string>
    <!-- As in, "A a file from this device". -->
    <string name="filter_name_file_unknown">с этого устройства</string>
    <!-- Name of a tab for adding a new host to the blacklist / whitelist. -->
    <string name="filter_edit_name">Хост</string>
    <!-- An example format of a hostname. -->
    <string name="filter_edit_name_placeholder">example.com</string>
    <string name="filter_edit_name_enter">Введите имя хоста</string>
    <string name="filter_edit_name_error">Это выглядит неправильно. Убедитесь, что вы вводите имя домена, например example.com.</string>
    <!-- Name of a tab that allows to add a file with hosts. -->
    <string name="filter_edit_file">Файл</string>
    <string name="filter_edit_file_choose">Выберите файл с хостами</string>
