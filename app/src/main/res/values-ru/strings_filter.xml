    <plurals name="filter_whitelist_text">
        <item quantity="one">%d в белом списке</item>
        <item quantity="other">%d в белом списке</item>
        <item quantity="few">%d в белом списке</item>
        <item quantity="many">%d в белом списке</item>
    </plurals>
