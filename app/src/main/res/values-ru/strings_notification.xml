<?xml version="1.0"?>
<resources>
    <string name="notification_on_text">Уведомления</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_on_desc">Когда включено, %s будет показывать уведомление каждый раз при блокировке рекламы.</string>
    <!-- Title of the notification that shows up every time ad is blocked. -->
    <string name="notification_blocked_title">Реклама заблокирована</string>
    <!-- Content of the notification that shows up every time ad is blocked. Parameter: blocked host name. -->
    <string name="notification_blocked_text">%s</string>
    <!-- Button to turn off notifications. -->
    <string name="notification_blocked_off">Не показывать</string>
    <!-- Button to whitelist a host that was just blocked, visible in a notification. -->
    <string name="notification_blocked_whitelist">В белый список</string>
    <!-- Toast message that shows up once user adds a host to the whitelist, using a button from the notification. -->
    <string name="notification_blocked_whitelist_applied">Готово! Изменения вступят в силу через несколько секунд.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_disabled">%s не будет отображать уведомления о блокировке рекламы.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_enabled">%s будет отображать уведомление при каждой блокировке рекламы.</string>
    <string name="notification_keepalive_text">Держать активным</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_desc">Когда включено, %s будет использовать методы, помогающие пережить очистку памяти.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_disabled">%s больше не защищено от экономии батареи или оперативной памяти.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_enabled">%s теперь будет защищать себя от выключения оперативной системой.</string>
    <!-- The ads blocked counter, displayed in the Keep Alive notification. Parameter: ads blocked counter. -->
    <plurals name="notification_keepalive_title">
        <item quantity="one">Одна реклама заблокирована</item>
        <item quantity="other">%d реклам заблокировано</item>
        <item quantity="few">%d рекламы заблокировано</item>
        <item quantity="many">%d реклам заблокировано</item>
    </plurals>
    <!-- The most recently blocked host, displayed in the Keep Alive notification. Parameter: blocked host name -->
    <string name="notification_keepalive_content">Последнее: %s</string>
    <!-- Content of the Keep Alive notification, if nothing was blocked yet, as in "Last: nothing yet". -->
    <string name="notification_keepalive_none">пока ничего</string>
</resources>
