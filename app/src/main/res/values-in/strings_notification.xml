<?xml version="1.0"?>
<resources>
    <string name="notification_on_text">Notifikasi</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_on_desc">Bila diaktifkan, %s akan menampilkan pemberitahuan setiap kali iklan diblokir.</string>
    <!-- Title of the notification that shows up every time ad is blocked. -->
    <string name="notification_blocked_title">Iklan diblokir</string>
    <!-- Content of the notification that shows up every time ad is blocked. Parameter: blocked host name. -->
    <string name="notification_blocked_text">%s</string>
    <!-- Button to turn off notifications. -->
    <string name="notification_blocked_off">Jangan tampilkan</string>
    <!-- Button to whitelist a host that was just blocked, visible in a notification. -->
    <string name="notification_blocked_whitelist">Daftar putih</string>
    <!-- Toast message that shows up once user adds a host to the whitelist, using a button from the notification. -->
    <string name="notification_blocked_whitelist_applied">Selesai! Perubahan bisa memakan waktu beberapa detik.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_disabled">%s tidak akan menampilkan pemberitahuan iklan</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_enabled">%s sekarang akan menampilkan notifikasi setiap kali iklan diblokir</string>
    <string name="notification_keepalive_text">Tetap hidupkan</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_desc">Bila diaktifkan, %s akan menggunakan teknik yang membantu untuk bertahan hidup dalam pembersihan memori.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_disabled">%s tidak terlindungi dari penyimpanan baterai atau memori lagi.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_enabled">%s sekarang akan melindungi dirinya dari dinonaktifkan oleh sistem operasi.</string>
    <!-- The ads blocked counter, displayed in the Keep Alive notification. Parameter: ads blocked counter. -->
    <plurals name="notification_keepalive_title">
        <item quantity="other">%d iklan diblokir</item>
    </plurals>
    <!-- The most recently blocked host, displayed in the Keep Alive notification. Parameter: blocked host name -->
    <string name="notification_keepalive_content">Terakhir: %s</string>
    <!-- Content of the Keep Alive notification, if nothing was blocked yet, as in "Last: nothing yet". -->
    <string name="notification_keepalive_none">belum ada</string>
</resources>
